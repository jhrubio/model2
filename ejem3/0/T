FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }

    t_i
    {
        type            fixedValue;
        value           uniform 573;
    }

    t_o
    {
        type            fixedValue;
        value           uniform 273;
    }
}

